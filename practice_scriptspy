a=20
b=30
c=50
#average
x=sum(a,b,c)
y=x/3
print(y)
#exchange values
(a,b)=(5,6)
b=a
a=b+1
print(a)
print(b)
#expression
print(4+6*9-3/2)
print(-5**2+34>66+8/2*3)
#string
a="ram's hand"
print(a)
b="Hi,My name is Ram"
print(b)
#operators
k="Besant Technology"
k.split()
m="Besant Technology"
a=m[2:6:1]
print(a)
b=m[0:7:1]
print(b)
print(b*5)
